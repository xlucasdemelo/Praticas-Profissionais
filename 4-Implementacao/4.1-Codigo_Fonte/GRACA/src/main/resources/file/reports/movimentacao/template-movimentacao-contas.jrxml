		</variable>
		<variable name="total_valor" class="java.lang.Long" calculation="Sum">
			<variableExpression><![CDATA[$F{valor_efetivado}]]></variableExpression>
		</variable>
		<variable name="media_total" class="java.lang.Long" calculation="Average">
