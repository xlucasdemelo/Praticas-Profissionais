from public.casa_lar as casa_lar
left outer join public.repasse as repasse on repasse.casa_lar_id = casa_lar.id

where $X{IN, casa_lar.id, casasLaresId} AND repasse.casa_lar_id = casa_lar.id

group by casa_lar.id]]>
	</queryString>
