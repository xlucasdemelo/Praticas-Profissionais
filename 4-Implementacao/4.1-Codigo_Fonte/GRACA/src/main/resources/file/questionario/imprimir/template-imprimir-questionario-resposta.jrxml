	(
	(itemContrato.valor_mao_de_obra * itemServico.realizado)
	+ (itemContrato.valor_equipamento_material * itemServico.realizado)
	) as sub_total,

sum
	(
	(itemContrato.valor_mao_de_obra * itemServico.realizado)

	)as total_realizado_mdo,

sum
	(
	(itemContrato.valor_equipamento_material * itemServico.realizado)

	) as total_realizado_mat,

fornecedor.nome_responsavel_fornecedor as nome_responsavel_fornecedor,

usuario.nome as elaborador

from $P!{default_schema}.pagamento
left outer join $P!{default_schema}.contrato on contrato.id = pagamento.contrato_id
left outer join $P!{default_schema}.fornecedor on contrato.fornecedor_id = fornecedor.id
left outer join $P!{default_schema}.ordem_servico as os on os.pagamento_id = pagamento.id
left outer join $P!{default_schema}.item_servico as itemServico on itemServico.ordem_servico_id = os.id
left outer join $P!{default_schema}.item_contrato as itemContrato on itemContrato.id = itemServico.item_contrato_id
left outer join $P!{default_schema}.unidade_medida as un on un.id = itemContrato.unidade_medida_id
left outer join $P!{default_schema}.usuario on usuario.id = pagamento.usuario_elaborador_id

where pagamento.id = $P{pagamento_id}
group by pagamento.numero_medicao, pagamento.data_fechamento, contrato.numero, contrato.calcularinss, fornecedor.razao_social, fornecedor.nome_responsavel_fornecedor, contrato.porcentagem_reajuste,
usuario.nome]]>
	</queryString>
