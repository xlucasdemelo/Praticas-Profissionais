	<subDataset name="Table Dataset 1" uuid="1e8de900-0551-40ed-86ff-fe2e865c0010">
		<parameter name="pagamento_id" class="java.lang.Long"/>
		<parameter name="default_schema" class="java.lang.String"/>
		<queryString>
			<![CDATA[select distinct itemServico.ordem, itemContrato.descricao_completa, itemContrato.descricao_resumida, sum(itemServico.realizado) as quantidade_realizada,

CAST( substring( itemServico.ordem, 0 , strpos( itemServico.ordem, '.' ) ) as int)  as first_index,

 CAST( substring( itemServico.ordem, strpos( itemServico.ordem, '.' )+1, CAST(1000 AS int) ) AS int ) last_index,

sum((itemContrato.valor_mao_de_obra * itemServico.realizado) )as total_realizado_MDO,

sum((itemContrato.valor_equipamento_material * itemServico.realizado)) as total_realizado_MAT_EQUIP,

sum
(
(itemContrato.valor_mao_de_obra * itemServico.realizado)
+ (itemContrato.valor_equipamento_material * itemServico.realizado)
) as custo_total_realizado,


contrato.numero, pagamento.numero_medicao, un.sigla,
itemContrato.valor_equipamento_material as preco_unitario_MDO,
itemContrato.valor_mao_de_obra as preco_unitario_MAT

from $P!{default_schema}.pagamento
left outer join $P!{default_schema}.ordem_servico as os on os.pagamento_id = pagamento.id
left outer join $P!{default_schema}.item_servico as itemServico on itemServico.ordem_servico_id = os.id
left outer join $P!{default_schema}.item_contrato as itemContrato on itemContrato.id = itemServico.item_contrato_id
left outer join $P!{default_schema}.contrato on pagamento.contrato_id = contrato.id
left outer join $P!{default_schema}.unidade_medida as un on un.id = itemContrato.unidade_medida_id

where pagamento.id = $P{pagamento_id} AND itemServico.ativo is true
group by itemServico.ordem, itemContrato.descricao_completa,  itemContrato.descricao_resumida, contrato.numero, pagamento.numero_medicao, un.sigla,itemContrato.valor_equipamento_material, itemContrato.valor_mao_de_obra
order by first_index, last_index]]>
		</queryString>
		<field name="ordem" class="java.lang.String"/>
		<field name="descricao_completa" class="java.lang.String"/>
		<field name="quantidade_realizada" class="java.math.BigDecimal"/>
		<field name="first_index" class="java.lang.Integer"/>
		<field name="last_index" class="java.lang.Integer"/>
		<field name="total_realizado_mdo" class="java.math.BigDecimal"/>
		<field name="total_realizado_mat_equip" class="java.math.BigDecimal"/>
		<field name="custo_total_realizado" class="java.math.BigDecimal"/>
		<field name="numero" class="java.lang.String"/>
		<field name="numero_medicao" class="java.lang.String"/>
		<field name="sigla" class="java.lang.String"/>
		<field name="preco_unitario_mdo" class="java.math.BigDecimal"/>
		<field name="preco_unitario_mat" class="java.math.BigDecimal"/>
		<field name="descricao_resumida" class="java.lang.String"/>
		<variable name="variable1" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{custo_total_realizado}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="dataset2" uuid="e8f8eeee-f3aa-4581-9f0b-21b8774b8a58">
		<parameter name="id_pagamento" class="java.lang.Long"/>
		<queryString>
			<![CDATA[select distinct itemServico.ordem, itemContrato.descricao_completa, sum(itemServico.realizado) as quantidade_realizada,

sum((itemContrato.valor_mao_de_obra * itemServico.realizado) )as total_realizado_MDO,

sum((itemContrato.valor_equipamento_material * itemServico.realizado)) as total_realizado_MAT_EQUIP,

sum
(
(itemContrato.valor_mao_de_obra * itemServico.realizado)
+ (itemContrato.valor_equipamento_material * itemServico.realizado)
) as custo_total_realizado,


contrato.numero, contrato.calcularinss as calcular_inss, pagamento.numero_medicao, un.sigla,
itemContrato.valor_equipamento_material as preco_unitario_MDO,
itemContrato.valor_mao_de_obra as preco_unitario_MAT

from osm.pagamento
left outer join osm.ordem_servico as os on os.pagamento_id = pagamento.id
left outer join osm.item_servico as itemServico on itemServico.ordem_servico_id = os.id
left outer join osm.item_contrato as itemContrato on itemContrato.id = itemServico.item_contrato_id
left outer join osm.contrato on pagamento.contrato_id = contrato.id
left outer join osm.unidade_medida as un on un.id = itemContrato.unidade_medida_id

where pagamento.id = $P{id_pagamento}
group by itemServico.ordem, itemContrato.descricao_completa, contrato.numero, contrato.calcularinss, pagamento.numero_medicao, un.sigla,itemContrato.valor_equipamento_material, itemContrato.valor_mao_de_obra]]>
		</queryString>
		<field name="ordem" class="java.lang.String"/>
		<field name="descricao_completa" class="java.lang.String"/>
		<field name="quantidade_realizada" class="java.math.BigDecimal"/>
		<field name="total_realizado_mdo" class="java.math.BigDecimal"/>
		<field name="total_realizado_mat_equip" class="java.math.BigDecimal"/>
		<field name="custo_total_realizado" class="java.math.BigDecimal"/>
		<field name="numero" class="java.lang.String"/>
		<field name="calcular_inss" class="java.lang.Boolean"/>
		<field name="numero_medicao" class="java.lang.String"/>
		<field name="sigla" class="java.lang.String"/>
		<field name="preco_unitario_mdo" class="java.math.BigDecimal"/>
		<field name="preco_unitario_mat" class="java.math.BigDecimal"/>
	</subDataset>
